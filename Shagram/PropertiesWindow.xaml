<Window x:Class="Shagram.PropertiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shagram"
        mc:Ignorable="d"
        Title="Shagram" Height="316.383" Width="460.468"
        ResizeMode="NoResize"
        WindowState="Normal"
        WindowStartupLocation="CenterScreen" 
        x:Name="main_window">
    <Grid>
        <Button Content="Log Out" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="420" Click="Button_Click"/>
        <ScrollViewer HorizontalAlignment="Left" Height="199" Margin="10,35,0,0" VerticalAlignment="Top" Width="420">
            <TextBlock x:Name="license" TextWrapping="Wrap" />
        </ScrollViewer>
        <Label Content="(c) Copyright 2018 by Oleh Yaroshchuk (Syarol)" HorizontalAlignment="Left" Margin="10,239,0,0" VerticalAlignment="Top" Height="38" Width="279"/>

    </Grid>
</Window>
